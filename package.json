{
  "name": "nest-mysql-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run prebuild && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pm2:prod": "pm2 start ecosystem.config.js --env production",
    "pm2:dev": "pm2 start ecosystem.config.js --env development",
    "pm2:test": "pm2 start ecosystem.config.js --env test",
    "generate": "rm -rf src/migration && ts-node ./node_modules/.bin/typeorm migration:generate -n Test",
    "db": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "db1": "typeormCli init"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/swagger": "^4.5.7",
    "@nestjs/typeorm": "^7.0.0",
    "ali-oss": "^6.8.0",
    "alipay-sdk": "^3.0.8",
    "axios": "^0.20.0",
    "body-parser-xml": "^2.0.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "connect-history-api-fallback": "^1.6.0",
    "dotenv": "^8.2.0",
    "express-rate-limit": "^5.1.3",
    "helmet": "^3.22.0",
    "husky": "^4.3.7",
    "jimp": "^0.12.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.25.3",
    "mysql": "^2.18.1",
    "nestjs-config": "^1.4.6",
    "nestjs-redis": "^1.2.7",
    "node-sso": "^1.0.1",
    "node-xlsx": "^0.15.0",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^2.1.3",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "simp-node-auth": "0.0.1",
    "svg-captcha": "^1.4.0",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.24",
    "typeorm-model-generator-cli": "^1.0.1",
    "uuid": "^8.0.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.3",
    "@types/express-rate-limit": "^5.0.0",
    "@types/helmet": "0.0.47",
    "@types/jest": "25.1.4",
    "@types/node": "^13.9.1",
    "@types/node-xlsx": "^0.15.0",
    "@types/request-ip": "0.0.35",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
